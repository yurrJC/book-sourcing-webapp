# API Keys - Replace these with your actual keys
ISBNDB_API_KEY=your_isbndb_api_key_here

# eBay API Credentials 
# To find these values, go to your eBay Developer Account at https://developer.ebay.com/my/keys
# Copy these from your existing application or create a new one
EBAY_APP_ID=your_ebay_app_id_here            # Client ID / App ID
EBAY_CERT_ID=your_ebay_cert_id_here          # Client Secret / Cert ID
EBAY_DEV_ID=your_ebay_dev_id_here            # Dev ID (Optional for this implementation)
EBAY_REDIRECT_URI=your_redirect_uri_here     # OAuth Redirect URI (Optional for client credentials flow)
EBAY_MARKETPLACE_ID=EBAY_AU                  # Leave as EBAY_AU for Australia 